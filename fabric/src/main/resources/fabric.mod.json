{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Minerino",
  "description": "A Twitch Chat Mod aiming to provide feature similar to the popular twitch chat software Chatterino",
  "authors": [
    "M0oosik"
  ],
  "contact": {
    "sources": "https://github.com/Mooosik/minerino"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mooosik.minerino.Minerino",
      "net.mooosik.minerino.command.CommandInitializer"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.18.x"
  },
  "suggests": {
    "another-mod": "*"
  }
